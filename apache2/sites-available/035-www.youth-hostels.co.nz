#    SSLEngine on

#    SSLCertificateFile      /etc/apache2/ssl-cert/youth-hostels.co.nz.crt
#    SSLCACertificateFile    /etc/apache2/ssl-cert/youth-hostels.co.nz.pem
#    SSLCertificateKeyFile   /etc/apache2/ssl-cert/youth-hostels.co.nz.key

     GnuTLSEnable on
     GnuTLSExportCertificates on
     GnuTLSCacheTimeout 500
     GnuTLSCertificateFile  /etc/apache2/ssl-cert/youth-hostels.co.nz.crt
     GnuTLSClientCAFile     /etc/apache2/ssl-cert/youth-hostels.co.nz.pem
     GnuTLSKeyFile          /etc/apache2/ssl-cert/youth-hostels.co.nz.key

     #GnuTLSPriorities NONE:+AES-128-CBC:+ARCFOUR-128:+RSA:+SHA1:+MD5:+COMP-NULL:$
     GnuTLSPriorities      NORMAL

     # SSL Protocol Adjustments:
     BrowserMatch ".*MSIE.*" nokeepalive ssl-unclean-shutdown downgrade-1.0 force$

</VirtualHost>
